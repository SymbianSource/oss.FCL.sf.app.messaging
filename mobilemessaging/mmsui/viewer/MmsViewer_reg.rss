RESOURCE APP_REGISTRATION_INFO
    {
    app_file = "mmsviewer";
    localisable_resource_file = APP_RESOURCE_DIR"\\mmsviewer";
    localisable_resource_id = R_MMSVIEWER_LOCALISABLE_APP_INFO;

